<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
	<acl>
		<resources>
			<resource id="Magento_Backend::admin">
				<resource id="Magento_Backend::stores">
					<resource id="Magento_Backend::stores_settings">
						<resource id="Magento_Config::config">
							<resource id="Omnyfy_VendorSearch::config_omnyfy_vendorsearch" title="vendor_search"/>
						</resource>
					</resource>
				</resource>
				<resource id="Omnyfy_VendorSearch::SearchHistory" sortOrder="10" title="SearchHistory">
					<resource id="Omnyfy_VendorSearch::SearchHistory_save" sortOrder="10" title="Save SearchHistory"/>
					<resource id="Omnyfy_VendorSearch::SearchHistory_delete" sortOrder="20" title="Delete SearchHistory"/>
					<resource id="Omnyfy_VendorSearch::SearchHistory_update" sortOrder="30" title="Update SearchHistory"/>
					<resource id="Omnyfy_VendorSearch::SearchHistory_view" sortOrder="40" title="View SearchHistory"/>
				</resource>
			</resource>
		</resources>
	</acl>
</config>
