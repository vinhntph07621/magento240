<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing_data_source</item>
            <item name="deps" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing_data_source</item>
        </item>
        <item name="spinner" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_columns</item>
    </argument>

    <dataSource name="omnyfy_mcm_pending_payout_pending_order_listing_data_source">
        <argument name="dataProvider" xsi:type="configurableObject">
            <argument name="class" xsi:type="string">Omnyfy\Mcm\Ui\DataProvider\PendingPayout\Grid\PendingOrderDataProvider</argument>
            <argument name="name" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing_data_source</argument>
            <argument name="primaryFieldName" xsi:type="string">id</argument>
            <argument name="requestFieldName" xsi:type="string">vendor_id</argument>
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
                    <item name="update_url" xsi:type="url" path="mui/index/render"/>
                    <item name="filter_url_params" xsi:type="array">
                        <item name="vendor_id" xsi:type="string">*</item>
                    </item>
                    <item name="storageConfig" xsi:type="array">
                        <item name="indexField" xsi:type="string">id</item>
                    </item>
                </item>
            </argument>
        </argument>
        <argument name="data" xsi:type="array">
            <item name="js_config" xsi:type="array">
                <item name="component" xsi:type="string">Magento_Ui/js/grid/provider</item>
            </item>
        </argument>
    </dataSource>
    <container name="listing_top">
        <argument name="data" xsi:type="array">
            <item name="config" xsi:type="array">
                <item name="template" xsi:type="string">ui/grid/toolbar</item>
            </item>
        </argument>
        <bookmark name="bookmarks">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="storageConfig" xsi:type="array">
                        <item name="namespace" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing</item>
                    </item>
                </item>
            </argument>
        </bookmark>
        <component name="columns_controls">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="columnsData" xsi:type="array">
                        <item name="provider" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_columns</item>
                    </item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/controls/columns</item>
                    <item name="displayArea" xsi:type="string">dataGridActions</item>
                </item>
            </argument>
        </component>
        <filters name="listing_filters">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="columnsProvider" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_columns</item>
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.filters</item>
                    </item>
                    <item name="templates" xsi:type="array">
                        <item name="filters" xsi:type="array">
                            <item name="select" xsi:type="array">
                                <item name="component" xsi:type="string">Magento_Ui/js/form/element/ui-select</item>
                                <item name="template" xsi:type="string">ui/grid/filters/elements/ui-select</item>
                            </item>
                        </item>
                    </item>
                    <item name="childDefaults" xsi:type="array">
                        <item name="provider" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing.listing_top.listing_filters</item>
                        <item name="imports" xsi:type="array">
                            <item name="visible" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_columns.${ $.index }:visible</item>
                        </item>
                    </item>
                </item>
                <item name="observers" xsi:type="array">
                    <item name="column" xsi:type="string">column</item>
                </item>
            </argument>
        </filters>
        <massaction name="listing_massaction">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="selectProvider" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_columns.ids</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/tree-massactions</item>
                    <item name="indexField" xsi:type="string">id</item>
                </item> 
            </argument>
            <action name="move_to_payout" class="Omnyfy\Mcm\Ui\Component\MassAction\VendorOrder">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">move_to_payout</item>
                        <item name="label" xsi:type="string" translate="true">Move to Payout</item>
                        <item name="urlPath" xsi:type="url" path="omnyfy_mcm/pendingpayouts/massMoveToPayout" />
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Move to Payout</item>
                            <item name="message" xsi:type="string" translate="true">Are you sure to move to payout the selected orders?</item>
                        </item>
                    </item>
                </argument>
                             
            </action>
        </massaction>
        <paging name="listing_paging">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="storageConfig" xsi:type="array">
                        <item name="provider" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing.listing_top.bookmarks</item>
                        <item name="namespace" xsi:type="string">current.paging</item>
                    </item>
                    <item name="selectProvider" xsi:type="string">omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_listing.omnyfy_mcm_pending_payout_pending_order_columns.ids</item>
                </item>
            </argument>
        </paging>
    </container>
    <columns name="omnyfy_mcm_pending_payout_pending_order_columns">        
        <selectionsColumn name="ids">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="resizeEnabled" xsi:type="boolean">false</item>
                    <item name="resizeDefaultWidth" xsi:type="string">55</item>
                    <item name="indexField" xsi:type="string">id</item>
					<item name="headerTmpl" xsi:type="string">Omnyfy_Mcm/grid/columns/multiselect</item>
                </item>
            </argument>
        </selectionsColumn>

        <column name="order_increment_id">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="sortable" xsi:type="boolean">true</item>
                    <item name="sorting" xsi:type="string">desc</item>
                    <item name="label" xsi:type="string" translate="true">Order ID</item>
                    <item name="sortOrder" xsi:type="number">5</item>
                </item>
            </argument>
        </column>
		<column name="created_at" class="Magento\Ui\Component\Listing\Columns\Date" >
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">dateRange</item>
                    <item name="component" xsi:type="string">Magento_Ui/js/grid/columns/date</item>
                    <item name="dataType" xsi:type="string">date</item>
                    <item name="label" xsi:type="string" translate="true">Order Placed Date</item>
					<item name="sortOrder" xsi:type="number">10</item>
                </item>
            </argument>
        </column>
        <column name="total_with_shipping">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="filter" xsi:type="boolean">false</item>
                    <item name="label" xsi:type="string" translate="true">Vendor Subtotal (Incl. tax)</item>
                    <item name="sortOrder" xsi:type="number">15</item>
                </item>
            </argument>
        </column>
        <column name="order_status">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="filter" xsi:type="boolean">false</item>
                    <item name="label" xsi:type="string" translate="true">Order Status</item>
                    <item name="sortOrder" xsi:type="number">20</item>
                </item>
            </argument>
        </column>
        <column name="total_category_fee_incl_tax">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="sortable" xsi:type="boolean">false</item>
                    <item name="filter" xsi:type="boolean">false</item>
                    <item name="label" xsi:type="string" translate="true">Category Commissions (Incl. tax)</item>
                    <item name="sortOrder" xsi:type="number">30</item>
                </item>
            </argument>
        </column>
        <column name="total_seller_fee_incl_tax">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="boolean">false</item>
                    <item name="label" xsi:type="string" translate="true">Seller Fees (Incl. tax)</item>
                    <item name="sortOrder" xsi:type="number">40</item>
                    <item name="sortable" xsi:type="boolean">false</item>
                </item>
            </argument>
        </column>
        
        <column name="total_disbursement_fee_incl_tax">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="boolean">false</item>
                    <item name="label" xsi:type="string" translate="true">Disbursement Fees (Incl. tax)</item>
                    <item name="sortOrder" xsi:type="number">50</item>
                    <item name="sortable" xsi:type="boolean">false</item>
                </item>
            </argument>
        </column>
        <column name="payout_shipping">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="boolean">false</item>
                    <item name="label" xsi:type="string" translate="true">Shipping Paid By Customer (Incl. tax)</item>
                    <item name="sortOrder" xsi:type="number">55</item>
                    <item name="sortable" xsi:type="boolean">false</item>
                </item>
            </argument>
        </column>
        <column name="payout_amount">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="boolean">false</item>
                    <item name="label" xsi:type="string" translate="true">Vendor Payout (Incl. tax)</item>
                    <item name="sortOrder" xsi:type="number">60</item>
                    <item name="sortable" xsi:type="boolean">false</item>
                </item>
            </argument>
        </column>
        
        <actionsColumn name="actions" class="Omnyfy\Mcm\Ui\Component\Listing\Column\PendingOrderAction">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="indexField" xsi:type="string">id</item>
                    <item name="urlEntityParamName" xsi:type="string">id</item>
                    <item name="dataType" xsi:type="string">actions</item>
                </item>
            </argument>
        </actionsColumn>        
    </columns>
</listing>
