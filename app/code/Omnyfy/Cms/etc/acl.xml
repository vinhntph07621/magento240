<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="Omnyfy_Cms::elements" title="Cms" sortOrder="10">
                        <resource id="Omnyfy_Cms::article" title="Article" sortOrder="10" />
                        <resource id="Omnyfy_Cms::category" title="Categories" sortOrder="20" />
                        <resource id="Omnyfy_Cms::import" title="Import" sortOrder="30" />
                    </resource>
					<resource id="Omnyfy_Cms::country_industry" title="Countries and Industries" sortOrder="20">
                        <resource id="Omnyfy_Cms::country" title="Countries" sortOrder="30" />
                        <resource id="Omnyfy_Cms::industry" title="Industries" sortOrder="50" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Omnyfy_Cms::config_cms" title="Cms Section" />
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
