<?xml version="1.0"?>

<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../app/code/Magento/Webapi/etc/webapi.xsd">
    <route url="/V1/demo/:customerId" method="GET">
        <service class="Omnyfy\Membership\Api\CustomRepositoryInterface" method="getById"/>
        <resources>
            <resource ref="self"/>
        </resources>
    </route>
    <route url="/V1/demo/:customerId/membership" method="GET">
        <service class="Omnyfy\Membership\Api\CustomRepositoryInterface" method="getMembershipType"/>
        <resources>
            <resource ref="self"/>
        </resources>
    </route>
</routes>