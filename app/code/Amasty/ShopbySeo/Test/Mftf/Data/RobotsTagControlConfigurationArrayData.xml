        <data key="AddRelNofollowSwitcher">0</data>
    </entity>
    <entity name="enableNoindexForPaginatedPagesSwitcher" type="rtcscenario" extends="robotsTagControlDefault">
        <data key="NoindexForPaginatedPagesSwitcher">1</data>
    </entity>

</entities>

