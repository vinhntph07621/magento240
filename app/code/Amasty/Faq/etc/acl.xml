<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::content">
                    <resource id="Amasty_Faq::faq" title="Amasty Faq">
                        <resource id="Amasty_Faq::question" title="Amasty Faq Questions Page" />
                        <resource id="Amasty_Faq::category" title="Amasty Faq Categories Page" />
                        <resource id="Amasty_Faq::reports" title="Amasty Faq Search Terms Report" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Amasty_Faq::config" title="Amasty Faq Settings" sortOrder="21216" />
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Backend::system">
                    <resource id="Magento_Backend::convert">
                        <resource id="Amasty_Faq::faq_import" title="Amasty Import FAQ" sortOrder="60" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>