<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/amasty_faq/question" method="POST">
        <service class="Amasty\Faq\Api\QuestionRepositoryInterface" method="save"/>
        <resources>
            <resource ref="Amasty_Faq::question" />
        </resources>
    </route>
    <route url="/V1/amasty_faq/question/:questionId" method="PUT">
        <service class="Amasty\Faq\Api\QuestionRepositoryInterface" method="save"/>
        <resources>
            <resource ref="Amasty_Faq::question" />
        </resources>
    </route>
    <route url="/V1/amasty_faq/question/:questionId" method="DELETE">
        <service class="Amasty\Faq\Api\QuestionRepositoryInterface" method="deleteById"/>
        <resources>
            <resource ref="Amasty_Faq::question" />
        </resources>
    </route>
    <route url="/V1/amasty_faq/question" method="GET">
        <service class="Amasty\Faq\Api\QuestionRepositoryInterface" method="getById"/>
        <resources>
            <resource ref="Amasty_Faq::question" />
        </resources>
    </route>
    <route url="/V1/amasty_faq/question/all" method="GET">
        <service class="Amasty\Faq\Api\QuestionRepositoryInterface" method="getList" />
        <resources>
            <resource ref="Amasty_Faq::question" />
        </resources>
    </route>

