    </route>
    <route url="/V1/amasty_faq/tag/:tagId" method="DELETE">
        <service class="Amasty\Faq\Api\TagRepositoryInterface" method="deleteById"/>
        <resources>
            <resource ref="Amasty_Faq::faq" />
